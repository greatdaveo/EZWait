{
  "expo": {
    "name": "EZWAit App",
    "slug": "expo-router-typescript",
    "scheme": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app-icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "backgroundColor": "#222"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yourcompany.ezwait"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app-icon.png",
        "backgroundColor": "#222"
      },
      "package": "com.haytherecharlie.exporoutertypescript"
    },
    "web": {
      "favicon": "./src/assets/images/app-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset"
    ],
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
